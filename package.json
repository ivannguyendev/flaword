{
  "name": "@whthduck/flaword",
  "version": "0.0.5",
  "description": "A library validate and prevent harmful request if which like a SQL injection, NoSQL injection, Javascript injection",
  "homepage": "https://github.com/whthduck/flaword#readme",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "clean": "rimraf dist *tsbuildinfo",
    "build": "APP_VERSION=$npm_package_version rimraf dist *tsbuildinfo && tsc --build",
    "build:docker": "tsc --build",
    "test": "ava"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:whthduck/flaword.git"
  },
  "keywords": [
    "SQL injection",
    "NoSQL injection",
    "harmful",
    "validation",
    "Javascript injection",
    "nodejs"
  ],
  "dependencies": {
    "debug": "^4.3.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/debug": "^4.1.7",
    "@types/lodash": "^4.14.182",
    "@types/node": "~12",
    "ava": "^4.3.0",
    "eslint": "~7.10",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.22.1",
    "rimraf": "~3.0",
    "ts-loader": "~6.1",
    "ts-node": "~8.4",
    "tsconfig-paths": "~3.9"
  },
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "dist/"
      },
      "compile": false
    }
  }
}
